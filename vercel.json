{
    "version": 2,
    "builds": [
      { "src": "api/app.py", "use": "@vercel/python" },
      { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "build" } }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "api/app.py" },
      { "src": "^/static/(.*)", "dest": "/static/$1" },
      { "src": ".*", "dest": "/index.html" }
    ]
  }